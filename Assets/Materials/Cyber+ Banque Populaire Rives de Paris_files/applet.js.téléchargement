function ATEApplet(t,e,i,a,s,r,n){this.appletClassName=t,this.archive=e,this.height=i,this.width=a,this.params=s,this.minVersion=r,this.alternateMessage=n}ATEApplet.CODE_PARAM="code",ATEApplet.ARCHIVE_PARAM="archive",ATEApplet.MAYSCRIPT_PARAM="mayscript",ATEApplet.PLUGIN_NAME="Java",ATEApplet.DEFAULT_CLASSID="clsid:8AD9C840-044E-11D1-B3E9-00805F499D93",ATEApplet.prototype.constructor=ATEApplet,ATEApplet.prototype={getClassId:function getClassId(){return null==this.classid?ATEApplet.DEFAULT_CLASSID:this.classid},getAppletClassName:function getAppletClassName(){return this.appletClassName},getArchive:function getArchive(){return this.archive},getHeight:function getHeight(){return this.height},getWidth:function getWidth(){return this.width},getParams:function getParams(){return this.params},getMinVersion:function getMinVersion(){return this.minVersion},getAlternateMessage:function getAlternateMessage(){return this.alternateMessage},writeParam:function writeParam(t,e){var i='<param name="';return i+=t,i+='" value="',i+=e,i+='" />'},writeParams:function writeParams(t){for(var e=this.getParams(),i=e.length,a=0;a<i;a++){var s=e[a];t+=this.writeParam(s.key,s.value)}return t},write:function write(t,e){if(-1!=PluginDetect.isMinVersion(ATEApplet.PLUGIN_NAME,this.getMinVersion())){var i="\x3c!--[if !IE]>--\x3e";i+='<object type="application/x-java-applet" height="',i+=this.getHeight(),i+='" width="',i+=this.getWidth(),i+='">',i+=this.writeParam(ATEApplet.CODE_PARAM,this.getAppletClassName()),i+=this.writeParam(ATEApplet.ARCHIVE_PARAM,this.getArchive()),i+=this.writeParam(ATEApplet.MAYSCRIPT_PARAM,"true"),i=this.writeParams(i),i+='"\x3c!--<![endif]--\x3e',i+='<object classid="',i+=this.getClassId(),i+='" height="',i+=this.getHeight(),i+='" width="',i+=this.getWidth(),i+='">',i+=this.writeParam(ATEApplet.CODE_PARAM,this.getAppletClassName()),i+=this.writeParam(ATEApplet.ARCHIVE_PARAM,this.getArchive()),i=this.writeParams(i),i+="</object>",i+="\x3c!--[if !IE]>--\x3e</object>\x3c!--<![endif]--\x3e",t.document.getElementById(e).innerHTML=i}else this.writeJavaAlternateContent(t,e)},writeJavaAlternateContent:function writeJavaAlternateContent(t,e){t.document.getElementById(e).innerHTML=this.getAlternateMessage()}};
