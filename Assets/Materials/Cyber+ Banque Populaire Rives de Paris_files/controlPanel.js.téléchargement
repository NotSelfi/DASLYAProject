var RESIZE_MAX_RETRY=20,resizeTimeoutCpt=RESIZE_MAX_RETRY;function getCPContentHeight(){var e=document.getElementById("cpc"),t=e.scrollHeight;return null==e?null:t}function resizeCP(){if(0==document.body.offsetHeight&&resizeTimeoutCpt>0)resizeTimeoutCpt--,window.setTimeout("rtff()",125);else{var e=document.getElementById("cpc"),t=document.body.offsetHeight;e.style.maxHeight=t>0?t+"px":"auto",resizeTimeoutCpt=RESIZE_MAX_RETRY}}var rtff=resizeCP;function resizeCPIE(){return getCPContentHeight()>document.documentElement.clientHeight?document.body.clientHeight+"px":"auto"}var rtie=resizeCPIE;
