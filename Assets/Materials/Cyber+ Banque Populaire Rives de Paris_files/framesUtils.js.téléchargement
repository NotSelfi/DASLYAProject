function getContentHeight(in_document){if(null==in_document)return null;var l_height=null,isIE=navigator.userAgent.toLowerCase().indexOf("msie")>-1||navigator.userAgent.toLowerCase().indexOf("trident")>-1;if(isIE){if(l_height=in_document.body.scrollHeight,in_document.documentMode>=8||null==in_document.documentMode){var l_marginCompensation=eval(in_document.body.topMargin)+eval(in_document.body.bottomMargin);l_height+=l_marginCompensation}}else{var l_height=in_document.body.offsetHeight,l_height_docEl=in_document.documentElement.offsetHeight,l_offsetHeightCompensation;if(Number.isInteger(l_height)&&Number.isInteger(l_height_docEl)&&(l_height>0||l_height_docEl>0))l_height=Math.max(l_height,l_height_docEl);else l_offsetHeightCompensation="INTERNET_89C3"==TW().portal.integrationMode?55:35,0==l_height||null==l_height||null==l_height?(l_height=l_height_docEl,0==l_height||null==l_height||null==l_height?l_height=in_document.documentElement.scrollHeight:l_height+=l_offsetHeightCompensation):l_height+=l_offsetHeightCompensation}return l_height}
