function resizeTableHeight(e,t,r){document.addEventListener("DOMContentLoaded",(function(){var a=document.getElementById(e).rows,n=a.length-1;if(n>0){var l=a[0].offsetHeight+2,o=a[1].offsetHeight,c="overflow-y:auto;";n<r&&(r=n,c=""),c+="max-height:"+(l+o*r)+"px;",document.getElementById(t).setAttribute("style",c)}}),!1)}function matchSearch(e,t){if(""===$.trim(e.term))return t;if(void 0===t.text)return null;for(var r=e.term.toLowerCase(),a="";a!==r;)a=r,r=r.replace(/([0-9]+) ([0-9]+)/,"$1$2");var n=r.split(" ").filter((function(e){return""!==e})),l=/(\S+(\s\S+)+)+(,\S+)*/g,o=t.text,c=o.match(l)?o.match(l).map((function(e){return e.replaceAll(/\s/g,"")})):"";c.length>0&&(o=o+" - "+c.filter((function(e){return e.length>0})).join(" - "));for(var i=0;i<n.length;i++)if(-1==o.toUpperCase().indexOf(n[i].toUpperCase()))return null;return t}function lstDataFilter(e,t,r,a){null!=r&&""!==r?document.addEventListener("DOMContentLoaded",(function(){var t=$(e).select2({placeholder:a,matcher:matchSearch,language:"fr",width:r}).parent();t.hasClass("aslbwf")&&(t=t.parent()),t.addClass("filterable-select")}),!1):document.addEventListener("DOMContentLoaded",(function(){$(e).select2({placeholder:a,matcher:matchSearch,language:"fr"})}),!1),document.addEventListener("DOMContentLoaded",(function(){$(e).on("select2:select",(function(){var r,a=e.substring(1),n=document.getElementById(a);"function"==typeof Event?r=new Event("change"):(r=document.createEvent("Event")).initEvent("change",!0,!0),n.dispatchEvent(r),document.getElementById("select2-"+a+"-container").setAttribute("title",t)})),document.getElementById("select2-"+e.substring(1)+"-container").setAttribute("title",t)}),!1)}function sortRows(e,t,r){for(var a=function parseDate(e){var t=e.trim().match(/^(\d+)[-/](\d+)[-/](\d+)$/);return t?new Date(parseInt(t[3],10),parseInt(t[2],10)-1,parseInt(t[1],10)):null},n=!0,l=!0,o=[],c=e.querySelectorAll("tbody tr"),i=0;i<c.length;i++){var s=c[i].querySelector("td:nth-child("+(t+1)+")").childNodes