function attachTableRowEvents(e,t,l,n,i,d){var c=document.getElementById(e),o=!1;if(null!=c){for(var r=0,s=null,u=new Array,a=0,h=c.rows.length;a<h;a++)if((s=c.rows[a]).cells.length>0&&"TH"==s.cells[0].tagName&&null!=d&&d.length>0){for(var g=0,f=s.cells.length;g<f;g++)for(var v=0,m=d.length;v<m;v++)if(s.cells[g].id==d[v][0]){u[u.length]=[g,d[v]];break}o=u.length>0}else null!=s.id&&(attachSelectionVisualisationEventsOnRow(s,t,l),o&&attachActiveSelectionEventsOnRow(e,n,i,u,s,t),"MS"==t&&(s.cells[s.cells.length-1].childNodes[0].checked||s.cells[s.cells.length-1].childNodes[0].disabled||"true"==s.cells[s.cells.length-1].childNodes[0].disabled)&&r++);if(r==c.rows.length-1){var y=c.rows[0].cells.length-1;invertCheckBoxStyle(c.rows[0].cells[y],!0)}}}var atre=attachTableRowEvents;function findChildOfType(e,t,l,n){var i=null,d=null;if(null!=e&&e.hasChildNodes())for(var c=e.childNodes.length-1;null==d&&c>=0;c--)null!=(i=e.childNodes[c])&&(i.tagName!=t||null!=n&&i.type==n||null!=l&&i.type!=l?i.hasChildNodes()&&(d=findChildOfType(i,t,l)):d=i);return d}function findAllChildsOfType(e,t,l,n){var i=null,d=new Array;if(null!=e&&e.hasChildNodes())for(var c=e.childNodes.length-1;c>=0;c--)if(null!=(i=e.childNodes[c]))if(i.tagName!=t||null!=n&&i.type==n||null!=l&&i.type!=l){if(i.hasChildNodes())for(var o=findAllChildsOfType(i,t,l),r=0;r<o.length;r++)d[d.length]=o[r]}else d[d.length]=i;return d}function attachActiveSelectionEventsOnRow(e,t,l,n,i,d){for(var c=i.id.substring(e.length+1),o=null,r=null,s=0,u=n.length;s<u;s++)if(null!=(o=findAllChildsOfType(i.cells[n[s][0]],"A"))&&o.length>0)for(var a=o.length,h=0;a>h;h++){var g;g="astr(event, '"+d+"','"+t+"','"+l+"','"+c+"', '"+n[s][1][1]+"','"+n[s][1][2]+"'"+(null!=(r=n[s][1][3])?',"'+r+'"':"")+");return false;",TW().attEvt(window,o[h],"click",g,!1)}}function attachSelectionVisualisationEventsOnRow(e,t,l){var n=TW();"SS"==t?n.attEvt(window,e.cells[e.cells.length-1].childNodes[0],"click","sstr(arguments[0], '"+e.id+"')",!1):"MS"==t&&(isLineSelectionDisabled(e)||n.attEvt(window,e.cells[e.cells.length-1].childNodes[0],"click","mstr(arguments[0],'"+e.id+"', "+l+")",!1)),l&&(n.attEvt(window,e,"mouseover","thst('"+e.id+"')",!1),n.attEvt(window,e,"mouseout","tust('"+e.id+"')",!1))}function toggleSelectionWidgetInRow(e,t,l){var n,i=TW().getEventTarget(window,l),d=findChildOfType(e,"INPUT",t);null!=i&&null!=d&&("checked"!==d.getAttribute("checked")&&""!==d.getAttribute("checked")||("checked"===d.getAttribute("checked")?(d.removeAttribute("checked"),n=!1):(d.setAttribute("checked","checked"),n=!0),d.checked=n),"true"!==d.getAttribute("checked")&&"false"!==d.getAttribute("checked")||(n="true"!==d.getAttribute("checked"),d.setAttribute("checked",n),d.checked=n))}function ssr(e,t){var l=document.getElementById(e);if(null!=l){var n=findChildOfType(l,"TBODY");null!=n&&n.setAttribute("selRowId",t)}}function sstr(e,t){var l=TW(),n=document.getElementById(t);if(null!=n&&!l.isSelectedStyle(n)){var i=n.parentNode,d=i.getAttribute("selRowId");if(null!=d){var c=document.getElementById(d);null!=c&&(l.toggleSelectedStyle(c),toggleSelectionWidgetInRow(c,"radio",e))}l.toggleSelectedStyle(n),toggleSelectionWidgetInRow(n,"radio",e),i.setAttribute("selRowId",n.id),selectionHook(getTableIdWithRow(n))}}function mstr(e,t,l){var n=document.getElementById(t);null!=n&&(TW().toggleSelectedStyle(n,l),toggleSelectionWidgetInRow(n,"checkbox",e),selectionHook(getTableIdWithRow(n)))}function sall(e,t){for(var l=document.getElementById(e),n=null!=l?l.rows:null,i=null!=n?n.length:0,d=i>0?n[0].cells.length-1:0,c=null,o=!1,r=1;r<i;r++){var s=n[r];if(null!=(c=findChildOfType(s.cells[d],"INPUT","checkbox"))){o=c.disabled||"true"==c.disabled;var u="none"==s.style.display;if(!o&&!u)c.checked!=t&&(TW().toggleSelectedStyle(s,!1),c.checked=t,c.setAttribute("checked",t))}}invertCheckBoxStyle(n[0].cells[d],t),selectionHook(e),updateNumChecked(l)}function updateNumChecked(e){var t=document.getElementById(e.id+"Recherchelabel"),l=0;if(e.querySelector('img[id$="_allON"]')){for(var n=e.querySelectorAll("tbody tr"),i=0;i<n.length;i++)n[i].getElementsByClassName("cbxTab")[0].checked&&l++;t.innerHTML="Nombre de lignes sélectionnées: "+l}}function astr(e,t,l,n,i,d,c,o){return"null"!=n?uds(n,i,"AS"):uds(l,i,t),sab(d,c,o),TW().stopEventPropagation(e),!1}function thst(e){var t=document.getElementById(e);null!=t&&TW().highlightStyle(t)}function tust(e){var t=document.getElementById(e);null!=t&&TW().unhighlightStyle(t)}function updateSelection(e,t,l){if(null!=t)for(var n=document.getElementsByName(e),i=0;i<n.length;i++){if("AS"==l&&"hidden"==n[i].type){n[i].value=t;break}"SS"==l&&"radio"==n[i].type&&n[i].value==t&&(n[i].checked=!0),"MS"==l&&"checkbox"==n[i].type&&n[i].value==t&&(n[i].disabled||"true"==n[i].disabled||(n[i].checked=!0))}}var uds=updateSelection;function onChangeFilter(e,t){var l=document.getElementById("myForm");if(void 0!==l){l.action=e;var n=l.elements.validationStrategy;null!=n&&null!=t&&(n.value=t),null!=this.preSubForm&&this.preSubForm(),l.submit()}}var ocf=onChangeFilter;function selectActionSort(e){var t=document.getElementById("myForm");void 0!==t&&(t.action=e,null!=this.preSubForm&&this.preSubForm(),t.submit())}var sas=selectActionSort;function getTableIdWithRow(e){var t=e.id;return t.substring(0,t.indexOf("_"))}function isLineSelected(e){var t=!1;if(e){var l=e.cells.length-1,n=findChildOfType(e.cells[l],"INPUT",null,"hidden");n&&(t=n.checked)}return t}function isLineSelectionDisabled(e){var t=!1;if(e){var l=e.cells.length-1,n=findChildOfType(e.cells[l],"INPUT",null,"hidden");n&&(t=n.disabled||"true"==n.disabled)}return t}function invertCheckBoxStyle(e,t){var l=e.childNodes[0],n=e.childNodes[1];t?(TW().maskStyle(l),TW().unmaskStyle(n)):(TW().maskStyle(n),TW().unmaskStyle(l))}
