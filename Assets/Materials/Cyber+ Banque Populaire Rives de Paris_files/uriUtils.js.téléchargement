function getPortal(){return getTW().portal}function getTW(){return null==getPortalWindow()?TW:getPortalWindow()}function startsWith(t,n){return null!=t&&t.length>0&&null!=n&&n.length>0&&0==t.indexOf(n)}function concatURI(t,n){return startsWith(n,"/")?t+n:t+"/"+n}function buildURI(t){return concatURI(getPortal().contextPath,t)}function buildURIFromSkinRef(t){return TW().SkinParameters.getParameter(t)}function buildImgURI(t){return buildURI(concatURI(getSkinImgPath(),t))}function buildCSSURI(t){return buildURI(concatURI(getSkinCSSPath(),t))}function buildJSURI(t){return buildURI(concatURI(getSkinJSPath(),t))}function buildPagesURI(t){return buildURI(concatURI(getSkinPagesPath(),t))}function buildActionURI(t){return buildURI(concatURI(getPortal().requestChannel,t))}function getSkinImgPath(){return getPortal().skinImgPath}function getSkinCSSPath(){return getPortal().skinCSSPath}function getSkinJSPath(){return getPortal().skinJSPath}function getSkinPagesPath(){return getPortal().skinPagesPath}function addURIParam(t,n,e){var r=t;return null!=t&&t.length>0&&(r+=t.indexOf("?")>0?"&":"?",r+=n+"="+(null==e?"null":e)),r}
